    "node-sass": "^9.0.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.4.21",
    "postcss-import": "^16.0.1",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.0",
